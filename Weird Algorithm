#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
const ll mod = 1e9+7;
 void solve(){
    ll n;
    cin>>n;
    while(n){
        cout<<n<<" ";
        if(n==1){
            break;
        }
        if(n%2==0){
            n= n/2;
        }
        else{
            n=3*n+1;
        }
    }
    cout<<endl;
 }
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    
        solve();
    
 
    return 0;
}
